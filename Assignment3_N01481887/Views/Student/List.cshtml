@model IEnumerable<Assignment3_N01481887.Models.Student>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page should be showing information about all the Students</p>
<div id="containerlist">
    <h2>List of Students</h2>
    @foreach(var student in Model)
            {
                <div class="list-group-item row">
                    <div class="col-md-12"><a href="/Student/Show/@student.StudentId">@student.StudentFname @student.StudentLname</a></div>

                </div>
            }

</div>

